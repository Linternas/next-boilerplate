{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "src/*": ["./*"],
      "pages/*": ["pages/*"],
      "hooks/*": ["hooks/*"],
      "store/*": ["redux/*"],
      "ui-component/*": ["ui-component/*"],
      "assets/*": ["assets/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  }
}
